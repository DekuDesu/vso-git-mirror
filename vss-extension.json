{
    "id": "gitmirror#{GitVersion.PreReleaseLabel}#",
    "manifestVersion": 1,
    "name": "Git Mirror #{GitVersion.PreReleaseLabel}#",
    "version": "#{GitVersion.Major}#.#{GitVersion.Minor}#.#{GitVersion.CommitsSinceVersionSource}#",
    "public": true,
    "publisher": "nobitagamer",
    "galleryFlags": [
        "#{Extension.galleryFlags}#"
    ],
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "description": "This task help you mirror your VSTS Git repository with another.",
    "categories": [
        "Build and release"
    ],
    "tags": [
        "release",
        "build",
        "git",
        "mirror",
        "GitHub",
        "Git Push",
        "Branch"
    ],
    "links": {
        "learn": {
            "uri": "http://vso-git-mirror.readthedocs.io"
        },
        "support": {
            "uri": "https://github.com/nobitagamer/vso-git-mirror.git/issues"
        }
    },
    "icons": {
        "default": "images/extension-icon.png"
    },
    "branding": {
        "color": "#1C3272",
        "theme": "dark"
    },
    "screenshots": [{
        "path": "images/screenshot-01.png"
    }],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE.txt"
        }
    },
    "files": [{
            "path": "gitmirror-task"
        }

    ],
    "contributions": [{
        "id": "gitmirror-task",
        "type": "ms.vss-distributed-task.task",
        "targets": [
            "ms.vss-distributed-task.tasks"
        ],
        "properties": {
            "name": "gitmirror-task"
        }
    }]
}